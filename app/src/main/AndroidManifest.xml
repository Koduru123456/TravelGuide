<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/ic_travelguide"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_travelguide"
        android:supportsRtl="true"
        android:theme="@style/Theme.TravelGuide"
        tools:targetApi="31">
        <activity
            android:name="hemanth.S3083018.travelguide.ui.theme.PlaceDetailsActivity"
            android:exported="false" />
        <activity
            android:name="hemanth.S3083018.travelguide.ui.theme.DestinationSearchActivity"
            android:exported="true"></activity>
        <activity
            android:name="hemanth.S3083018.travelguide.HomeActivity"
            android:exported="true"></activity>
        <activity
            android:name="hemanth.S3083018.travelguide.SignUpActivity"
            android:exported="false" />
        <activity
            android:name="hemanth.S3083018.travelguide.SignInActivity"
            android:exported="false"
            android:theme="@style/Theme.TravelGuide" />
        <activity
            android:name="hemanth.S3083018.travelguide.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.TravelGuide">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>